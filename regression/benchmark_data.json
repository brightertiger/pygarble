{
  "test_cases": [
    {
      "category": "normal_text",
      "description": "Clean English sentences",
      "cases": [
        {"text": "The quick brown fox jumps over the lazy dog.", "expected_garbled": false},
        {"text": "Hello, how are you doing today?", "expected_garbled": false},
        {"text": "Machine learning is transforming the technology industry.", "expected_garbled": false},
        {"text": "Please review the attached document and provide your feedback.", "expected_garbled": false},
        {"text": "The weather forecast predicts rain tomorrow afternoon.", "expected_garbled": false},
        {"text": "I'll meet you at the coffee shop around 3 PM.", "expected_garbled": false},
        {"text": "The project deadline has been extended by two weeks.", "expected_garbled": false},
        {"text": "Can you send me the report by end of day?", "expected_garbled": false},
        {"text": "The restaurant serves excellent Italian cuisine.", "expected_garbled": false},
        {"text": "We need to schedule a meeting to discuss the quarterly results.", "expected_garbled": false}
      ]
    },
    {
      "category": "normal_paragraphs",
